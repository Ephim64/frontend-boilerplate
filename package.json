{
  "name": "boilerplate-webpack",
  "private": true,
  "version": "0.0.1",
  "description": "Webpack configuration for RD unit",
  "main": "index.js",
  "scripts": {
    "build":
      "set NODE_ENV=production && ./node_modules/.bin/webpack --config ./config/webpack.config.js",
    "watch": "npm run build -- --watch",
    "start":
      "./node_modules/.bin/webpack-dev-server --config ./config/webpack.config.js --open"
  },
