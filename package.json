{
  "name": "boilerplate-webpack",
  "private": true,
  "version": "0.0.1",
  "description": "Webpack configuration for RD unit",
  "main": "index.js",
  "scripts": {
    "build": "set NODE_ENV=production & webpack --config ./config/webpack.config.prod.js",
    "watch": "npm run build -- --watch",
    "start": "set NODE_ENV=development & webpack-dev-server --config ./config/webpack.config.dev.js --open",
    "spritesmith": "spritesmith",
    "lint:html": "node scripts/htmllint ./src/index.html",
    "lint:css": "stylelint ./src/**/*.css",
    "lint:scss": "sass-lint -c .sasslintrc **/*.scss -v -q",
    "lint:js": "eslint ./src/**/*.js",
    "lint": "npm run lint:html&npm run lint:css&npm run lint:scss&npm run lint:js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ephim64/webpack-configs.git"
  },
  "author": "Ephim Gotsco",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ephim64/webpack-configs/issues"
  },
  "homepage": "https://github.com/ephim64/webpack-configs#readme",
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "chalk": "2.3.0",
    "css-loader": "0.28.7",
    "eslint": "4.9.0",
    "eslint-loader": "1.9.0",
    "extract-loader": "1.0.1",
    "extract-text-webpack-plugin": "3.0.1",
    "file-loader": "1.1.4",
    "html-loader": "0.5.1",
    "html-webpack-plugin": "2.30.1",
    "htmllint": "0.6.0",
    "htmllint-loader": "1.3.8",
    "node-sass": "4.5.3",
    "resolve-url-loader": "2.1.1",
    "sass-lint": "1.12.1",
    "sass-loader": "6.0.6",
    "spritesmith": "3.1.1",
    "spritesmith-cli": "1.1.2",
    "style-loader": "0.18.2",
    "stylelint": "8.1.1",
    "stylelint-webpack-plugin": "0.9.0",
    "url-loader": "0.6.2",
    "webpack": "3.7.1",
    "webpack-dev-server": "2.9.1",
    "webpack-merge": "4.1.0",
    "webpack-spritesmith": "0.3.3"
  }
}
