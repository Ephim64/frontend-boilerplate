    "css-loader": "0.28.7",
    "extract-loader": "1.0.1",
    "extract-text-webpack-plugin": "3.0.1",
    "file-loader": "1.1.4",
    "friendly-errors-webpack-plugin": "1.6.1",
    "html-loader": "0.5.1",
    "html-webpack-plugin": "2.30.1",
    "htmlhint-loader": "1.3.0",
    "htmllint-loader": "1.3.8",
    "node-sass": "4.5.3",
